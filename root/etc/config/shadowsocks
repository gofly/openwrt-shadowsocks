config servers main
	option server '12.34.56.78'
	option server_port '1234'
	option encrypt_method 'chacha20-ietf-poly1305'
	option password ''
	option plugin ''
	option plugin_opts ''
	option timeout '60'
	option fast_open '1'
	option no_delay '1'

config redir
	option main_server main

